{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": { 
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-unused-vars": 0,
        "no-new-wrappers":"off"
    }
    
}
